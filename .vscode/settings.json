{
    "cpptest.workingDirectory": "${workspaceFolder}/MyApplication",
    "cpptest.compilerIdentifier": "arm_linux_gnueabi-gcc9",
    "cpptest.commandLinePattern": "cpptestcli ${stopFile} ${compilerConfig} ${testConfig} ${reportDir} ${selectedFiles} -module . -settings ${workspaceFolder}/tools/cpptest/cpptestcli.properties -trace make clean all --cpptesttraceTraceCommand=arm-linux-gnueabi-g* " ,
    "cpptest.testConfiguration": "builtin://Recommended Rules",
    "cpptest.installationLocation": "/opt/cpptest",
}